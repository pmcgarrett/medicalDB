<h1> Admin Page </h1>
{{#each allUsers}}{{/each}}

<div class="card" style="width: 18rem;">
  <ul class="list-group list-group-flush">
    <li class="list-group-item">
      {{#each allUsers}}
        <h2> {{username}} </h2>
        <a href="/user/edit/{{_id}}"> edit</a>
        <form action="/user/delete/{{_id}}" method="post">
          <button type="submit">delete</button>
        </form>
      {{/each}}
    </li>

  </ul>
</div>